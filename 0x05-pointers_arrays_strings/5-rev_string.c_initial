#include "main.h"

/**
 * rev_string - prints string in reverse
 * @s: string
 */

void rev_string(char *s)
{
	int len, len_rev;

	len = 0;

	if (len >= 0)
	{
		while (s[len] != '\0')
		{
			len++;
			
			len_rev = len ;
		}

		while (len_rev >= 0)
		{
			putchar(s[len_rev]);
			len_rev--;
		}
	}
}
